		teaser_show : function() {
			(function tryHit ( delay, times ) {
				try {
					UOLPD.Audience.countEvent( {category:paywall.tm_category(),action:"impressao"}, true ) ;
				}
				catch ( e ) {
					if ( times > 0 ) {
						setTimeout( function () {
							tryHit( delay, --times ) ;
						}, delay ) ;
					}
				}
			} ( 50, 300 ) ) ;
			if ( $( "article#news div.content" ).size() > 0 ) {
				// Folha 5.4
				$( "article#news div.content" ).children().not( "p:first, #ad-180x150-1" ).remove() ;
				$( "article#news div.content" ).append( paywall.teaser() ) ;
			}
			else if ( $( "article#news.content" ).size() > 0 ) {
				//FSP v2
				$( "article#news.content" ).children().not( "header, .fine_line, .creditart, .origin, p:first" ).remove() ;
				$( "article#news.content" ).append( paywall.teaser() ) ;
			}
			else {
				// Ultimo recurso. Falha nos seletores/condicionais
				window.location.replace( "//www1.folha.uol.com.br/paywall/login.shtml?" + paywall.url() ) ;
			}

			if ( $( ".teaser-2-btn-login-svg.teaser-2-btn-login-facebook" ).css( "border-radius" ) !== "23px" ) {
				$( "head" ).append( "<link href='//www1.folha.uol.com.br/folha/furniture/paywall/teaser.css?mobile' type='text/css' rel='stylesheet'>" ) ;
			}

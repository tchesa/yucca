<script type="text/javascript">

    formValidador.addField('share-by-email', 'shareByEmailNome', 'O campo Nome é obrigatório.', 'required');
    formValidador.addField('share-by-email', 'shareByEmailEmail', 'Seu e-mail não é um endereço de e-mail válido.', 'email');
    formValidador.addField('share-by-email', 'shareByEmailEmail', 'O e-mail informado não é válido.', 'required');
    formValidador.addField('share-by-email', 'shareByEmailToEmail', 'Seu e-mail não é um endereço de e-mail válido.', 'email');
    formValidador.addField('share-by-email', 'shareByEmailToEmail', 'O e-mail informado não é válido.', 'required');
    formValidador.addField('share-by-email', 'shareByEmailSubject', 'O campo Assunto é obrigatório.', 'required');
    formValidador.addField('share-by-email', 'shareByEmailMsg', 'O campo Mensagem é obrigatório.', 'required');

    function closeShareDialog()
    {
        $('#shareByEmail').modal('hide');

        $('#shareByEmailNome').val('');
        $('#shareByEmailEmail').val('');
        $('#shareByEmailToEmail').val('');
        $('#shareByEmailMsg').val('');
        $('#divShareConfirm').hide();
        $('#actionsContactShare').show();
        $('#share-by-email-result').html();
    }

    function showShareByEmail()
    {
        if($('#recaptcha-share').html() == "") {
            if($('#recaptcha-contact').html() == "")
                formValidador.addField('share-by-email', 'g-recaptcha-response', 'A caixa de verificação não foi selecionada.', 'required');
            else
                formValidador.addField('share-by-email', 'g-recaptcha-response-1', 'A caixa de verificação não foi selecionada.', 'required');
            grecaptcha.render('recaptcha-share', {
              'sitekey' : '6LdvHB8UAAAAAKBlpj55JuY-rXV6YLEy_JALke-n'
            });
        }
        else
            grecaptcha.reset();
        $('#shareByEmail').modal('show');
    }

    function shareByEmail()
    {
        if(formValidador.checkFormFields('share-by-email', 'share-by-email-result'))
        {
            jQuery.ajax({
                        'type':'POST',
                        'beforeSend':function( data ){
                            $("#actionsShareByEmail").hide();
                            $('#share-by-email-result').html('<div><center><span><img alt=""  src="/images/img-loading.gif"></span></center></div>');
                        },
                        'data':{
                                'email':$("#shareByEmailEmail").val(),
                                'emailTo':$("#shareByEmailToEmail").val(),
                                'message':$("#shareByEmailMsg").val(),
                                'name':$("#shareByEmailNome").val(),
                                'subject':$("#shareByEmailSubject").val(),
                                'valid':true,
                                'eid':154097,
                                'g-recaptcha-response':grecaptcha.getResponse()
                                },
                        //'dataType':'json',
                        'url':'/site/sendEmailShare',
                        'cache':false,
                        'success':function(html){
                                    if(html === "ok") {
                                        $("#share-by-email-result").show().html("<div class=\"flash-note-green text-center\">Seu e-mail foi enviado com sucesso! <span class=\"dialogue-action-link\"><a href=\"javascript:closeShareDialog()\">Fechar</a></span></div>");
                                    } else {
                                        $("#share-by-email-result").show().html("<div class=\"flash-note-red text-center\"><span>Ooops...</span></div>");
                                        $("#actionsShareByEmail").show();
                                        $('#share-by-email-result').html('');
                                    }
                                },
                        'error':function(){ $("#share-by-email-result").show().html("<div class=\"flash-note-red text-center\"><span>Ooops...</span></div>"); $("#actionsShareByEmail").show(); }
            });
        }
    }
</script>

